# -*- mode: snippet -*-
# name: skeleton
# key: skeleton
# expand-env: ((yas-indent-line 'fixed))
# --
import Html exposing (..)
import Html.Attributes exposing (..)
import Html.Events exposing (..)

main =
    Html.program
    { init = init
    , update = update
    , view = view
    , subscriptions = subscriptions
    }

-- MODEL

type alias Model = {}

init = ({}, Cmd.none)

-- UPDATE

type Msg = Foo

update : Msg -> Model -> (Model, Cmd Msg)
update msg model =
    case msg of
        Foo -> (model, Cmd.none)

-- SUBSCRIPTIONS

subscriptions : Model -> Sub Msg
subscriptions model =
    Sub.none

-- VIEW

view : Model -> Html Msg
view model = div [] [ text "hello, world" ]
